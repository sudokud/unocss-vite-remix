{
  "name": "chetoui.dev",
  "version": "1.0.0",
  "description": "vite, remix, unocss starting template",
  "main": "root.tsx",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "unodev": "unocss \"./app/**/*.tsx\" --out-file=\"app/styles/uno.css\" --watch",
    "unobuild": "unocss \"./app/**/*.tsx\" --out-file=\"app/styles/uno.css\" --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/your-repo-name.git"
  },
  "keywords": [
    "vitejs",
    "remix",
    "react",
    "unocss",
    "build tools",
    "frontend"
  ],
  "author": "Hamza chetoui <hamza@chetoui.dev>",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/your-repo-name#readme",
  "dependencies": {
    "@remix-run/node": "^2.10.0",
    "@remix-run/react": "^2.10.0",
    "@remix-run/serve": "^2.10.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@unocss/eslint-config": "^0.61.3",
    "@unocss/preset-attributify": "^0.61.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.1.6",
    "unocss": "^0.61.0",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
